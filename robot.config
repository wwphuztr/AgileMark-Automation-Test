# Robot Framework Configuration File

[Paths]
# Define paths for test execution
tests_dir = tests
results_dir = results
images_dir = images
libraries_dir = libraries

[Execution]
# Default execution settings
default_timeout = 30s
screenshot_on_failure = True
log_level = INFO

[SikuliX]
# SikuliX specific settings
sikuli_jar_path = C:/sikulixide.jar
java_path = java
similarity_threshold = 0.8

[Reporting]
# Report generation settings
generate_html_report = True
generate_xunit_report = False
report_title = AgileMark Automation Test Report
